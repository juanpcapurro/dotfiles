#!/bin/zsh

xrandr \
  --output DP-1 --primary --mode 1920x1080 --rotate normal \
  --output HDMI-1 --mode 1920x1080 --left-of DP-1\
  --output HDMI-2 --off \
  --output eDP-1 --off\
  && notify-send "Docked at home!"|| notify-send "Unable to dock :("

xset s 0
xset dpms 0 0 0
xset dpms force on
i3-msg restart

