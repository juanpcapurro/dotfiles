#!/bin/sh

xrandr \
  --output DP-1-1 --mode 1920x1080 \
  --output DP-1-2 --off \
  --output DP-1-3 --mode 1920x1080 --primary --right-of DP-1-1 \
  --output HDMI-1 --off\
  --output DP-1 --off \
  --output DP-2 --off \
  --output eDP-1 --mode 1920x1080 --right-of DP-1-3 \
  && notify-send "Docked!"|| notify-send "Unable to dock :("

xset s 0
xset dpms 0 0 0
xset dpms force on

